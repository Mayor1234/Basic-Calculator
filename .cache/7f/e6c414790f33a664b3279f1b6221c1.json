\n  /* background-color: var(--light1);\n  color: var(--bg-dark1); */\n  border-radius: 8px 8px 0px 0px;\n}\n\n.input-screen {\n  padding-top: 1rem;\n  color: var(--dark1);\n}\n.output-screen {\n  align-self: flex-end;\n  font-size: 2.5rem;\n}\n\n.calc-buttons {\n  display: grid;\n  grid-template-columns: repeat(4, 1fr);\n  max-width: 100%;\n  height: 100%;\n}\n\n.calc-btn {\n  padding: 1.2rem;\n  text-align: center;\n  cursor: pointer;\n  font-size: 1.2rem;\n  border: 0.5px solid var(--light1);\n  background-color: var(--btn-color);\n  color: var(--dark2);\n  /* background-color: var(--dark);\n  color: var(--light1); */\n}\n\n.calc-btn:hover {\n  background-color: rgba(194, 191, 191);\n  color: rgb(229, 227, 227);\n}\n\n.calc-btn:nth-child(4n),\n.calc-btn:nth-child(-n + 3) {\n  color: var(--op-color);\n  font-size: 1.3rem;\n}\n.calc-btn:nth-child(4n):hover,\n.calc-btn:nth-child(-n + 3):hover {\n  color: var(--op-hover);\n}\n.calc-btn:nth-child(1) {\n  background-color: var(--c-color);\n  color: var(--dark2);\n}\n.calc-btn:nth-child(1):hover {\n  background-color: rgb(206, 80, 58);\n  color: var(--dark2);\n  border: 0.5px solid var(--op-color);\n}\n\n.equals {\n  border-radius: 0 0 8px 0;\n}\n.dot {\n  border-radius: 0 0 0 8px;\n}\n\n/* Theme toggle button */\n\n.theme-toggle {\n  position: relative;\n  display: flex;\n  justify-content: flex-end;\n  width: 200px;\n  top: 4px;\n  left: 1rem;\n}\nlabel {\n  position: absolute;\n  width: 20%;\n  height: 20px;\n  background-color: var(--light2);\n  border-radius: 50px;\n  cursor: pointer;\n}\ninput {\n  position: absolute;\n  display: none;\n}\n.slider {\n  position: absolute;\n  width: 19%;\n  height: 19%;\n  border-radius: 50px;\n  transition: 0.2s;\n}\n/* .slider:active {\n  transform: scale(0.95);\n} */\ninput:checked ~ .slider {\n  background-color: var(--light);\n  height: 20px;\n  width: 20px;\n}\n\n.slider::before {\n  content: '';\n  position: absolute;\n  top: 1px;\n  left: 1px;\n  width: 18px;\n  height: 18px;\n  border-radius: 50px;\n  box-shadow: inset 28px -4px 0px 0px var(--dark2);\n  transition: 0.3s;\n}\n\ninput:checked ~ .slider::before {\n  transform: translateX(20px);\n  background-color: var(--dark2);\n  box-shadow: none;\n}\n"},"lineCount":196}},"error":null,"hash":"9ffaf3f43c8a10cd398a9dc482adcdea","cacheData":{"env":{}}}